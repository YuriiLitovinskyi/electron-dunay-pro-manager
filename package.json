{
  "name": "dunay128-statistics",
  "productName": "Dunay-PRO-statistics",
  "version": "1.0.0",
  "description": "Application to get info from Dunay-PRO Firebird DB when devices Dunay-128 restarted for the first time",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --asar --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"dunay-pro-statistics\""
  },
  "author": "Yurii Litovinskyi",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.2.2",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "electron-packager": "^15.2.0",
    "moment": "^2.29.1",
    "nested-json-to-table": "^1.0.8",
    "node-firebird": "^0.9.9"
  }
}
